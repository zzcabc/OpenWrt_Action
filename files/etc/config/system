
config system
	option hostname 'ImmortalWrt'
	option ttylogin '0'
	option log_size '512'
	option urandom_seed '0'
	option timezone 'CST-8'
	option zonename 'Asia/Shanghai'
	option log_proto 'udp'
	option conloglevel '8'
	option cronloglevel '5'
	option log_file '/tmp/log/system.log'

config timeserver 'ntp'
	option enable_server '1'
	list server 'ntp.ntsc.ac.cn'
	list server 'cn.ntp.org.cn'
	list server 'edu.ntp.org.cn'
	list server 'ntp1.nim.ac.cn'
	list server 'ntp2.nim.ac.cn'
	list server 'cn.pool.ntp.org'
	list server 'ntp.aliyun.com'
	list server 'ntp.tencent.com'
	list server 'ntp.sjtu.edu.cn'
	list server 'ntp.shu.edu.cn'
	list server 'ntp.tuna.tsinghua.edu.cn'
	list server 'time.ustc.edu.cn'
	list server 'ntp.tuna.tsinghua.edu.cn'
	list server 'ntp.nju.edu.cn'
	list server 'time.windows.com'

config imm_init
	option ipv6 '2'
	option lang '1'
	option system_chn '1'
	option opkg_mirror 'https://mirrors.vsean.net/openwrt'
	option anon_mount '1'
	option packet_steering '1'

